# =========================================================================================
# CONFIGURACIONES DE COLORES
# =========================================================================================
HEADER = '\033[96m'
OKBLUE = '\033[94m'
OKGREEN = '\033[92m'
WARNING = '\033[93m'
FAIL = '\033[91m'
END = '\033[0m'
BOLD = '\033[1m'
UNDERLINE = '\033[4m'
PWD_DIR = pwd

all: build \
	otro

build:
	@+echo $(HEADER)"---------------------------------------------"$(END)
	@+echo $(HEADER)"Contruyendo la imagen"$(END)
	@+echo $(HEADER)"---------------------------------------------"$(END)

otro:
	@+echo $(HEADER)"---------------------------------------------"$(END)
	@+echo $(HEADER)"Otroooooooooooooooo"$(END)
	@+echo $(HEADER)"---------------------------------------------"$(END)